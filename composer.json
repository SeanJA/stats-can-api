{
    "name": "seanja/stats-can-api",
    "description": "api client to interact with the stats can api",
    "license": "LGPL-3.0-or-later",
    "type": "library",
    "keywords": ["statscan", "Web Data Service", "stats-can", "stats can", "wds", "wds canada", "canada"],
    "autoload": {
        "psr-4": {
            "SeanJA\\StatsCanAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SeanJA\\StatsCanAPI\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "seanja",
            "email": "code@seanja.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.8",
        "seanja/cacheable-trait": "^1.0",
        "symfony/cache": "^7.0",
        "ramsey/collection": "^2.0"

    },
    "scripts": {
        "coverage": "phpunit --coverage-html=./coverage --coverage-text"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "ext-simplexml": "*"
    }
}
